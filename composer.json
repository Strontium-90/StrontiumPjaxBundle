{
    "name": "strontium/pjax-bundle",
    "type": "symfony-bundle",
    "description": "Integration of defunkt/jquery-pjax into Symfony 2",
    "keywords": [
        "pjax",
        "Symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksey Bannov",
            "email": "a.s.bannov@gmail.com",
            "homepage": "https://github.com/Strontium-90"
        },
        {
            "name": "Anton Tyutin",
            "email": "anton@tyutin.ru",
            "homepage": "https://github.com/AntonTyutin"
        },
        {
            "name": "Tarasov Konstantin",
            "email": "terrasoff@terrasoff.ru",
            "homepage": "https://github.com/terrasoff"
        }
    ],
    "autoload": {
        "psr-4": {
            "Strontium\\PjaxBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.6",
        "symfony/twig-bundle": "~2.3",

        "components/jquery-pjax": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "phpspec/prophecy-phpunit": "~1.1"
    },
    "suggest": {
        "mopa/bootstrap-bundle": "~3.0"
    },
    "scripts": {},
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "components/jquery-pjax",
                "type": "component",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/Strontium-90/jquery-pjax.git",
                    "type": "git",
                    "reference": "master"
                },
                "extra": {
                    "name": "pjax",
                    "component": {
                        "scripts": [
                            "./jquery.pjax.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
