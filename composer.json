        "phpunit/phpunit": "~4.5",
        "phpspec/prophecy-phpunit": "~1.1"
    },
    "suggest": {
        "mopa/bootstrap-bundle": "~3.0"
    },
    "scripts": {},
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
